{
    "@rule": "House",
    "interior": {"@rule": "Frame", "content": "MainCorridor"},
    "exterior": {"@rule": "NOP"},
    "rect": "[0,0,80,40]",

    "MainCorridor": {
        "@rule": "Corridor",
        "left": ["NRooms", "NRooms", "NRooms", "NRooms"],
        "right": ["NRooms", "SideCorridor"],
        "corridor": "NOP"
    },
    "SideCorridor": {
        "@rule": "Corridor",
        "left": ["NRooms", "NRooms"],
        "right": ["NRooms", "NRooms"],
        "corridor": "NOP"
    },
    "NRooms": ["ThreeRooms", "TwoRooms", "Room"],
    "TwoRooms": {
        "@rule": "RoomLine",
        "rooms": ["Room", "Room"]
    },
    "ThreeRooms": {
        "@rule": "RoomLine",
        "rooms": ["Room", "Room", "Room"]
    },
    "Room": {
        "@rule": "Room"
    },
    "NOP": {
        "@rule": "NOP"
    }
}