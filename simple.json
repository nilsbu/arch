{
    "Root": {
        "@rule": "House",
        "interior": "Corridor",
        "exterior": {"@rule": "NOP"},
        "rect": "[0,0,40,20]"
    },
    "Corridor": {
        "@rule": "Corridor",
        "left": ["NRooms", "NRooms", "NRooms", "NRooms"],
        "right": ["NRooms", "NRooms"],
        "corridor": "Room"
    },
    "NRooms": ["ThreeRooms", "TwoRooms", "Room"],
    "TwoRooms": {
        "@rule": "RoomLine",
        "rooms": ["Room", "Room"]
    },
    "ThreeRooms": {
        "@rule": "RoomLine",
        "rooms": ["Room", "Room", "Room"]
    },
    "Room": {
        "@rule": "Room"
    }
}